{
  "entryFile": "src/App.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/features/**/*Controller.ts"
  ],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3
  },
  "routes": {
    "authenticationModule": "src/features/auth/Authentication.ts",
    "middleware": "express",
    "routesDir": "build",
    "iocModule": "src/base/tsyringe/ioc"
  }
}